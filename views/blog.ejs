<%- include('partials/header') -%>
<section class = "container">
    <h1>Blog v.1</h1>
    <% postsArr.forEach(function(element){ %>
        <h2><%=element.title%></h2>
        <p>
          <%=element.input.substring(0, 100) + "..."%>
          <a href="/blog/<%=element.title%>">Read More</a>
        </p>
      <%}) %>
</section>
